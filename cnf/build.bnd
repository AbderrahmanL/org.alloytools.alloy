src:          			src/main/java
bin:          			target/classes
testsrc:      			src/test/java
testbin:      			target/test-classes
target-dir:   			target
javac.source:           1.8
javac.target:           ${javac.source}
javac.compliance:       ${javac.source}
javac.debug:            on

Git-Descriptor:         	${system-allow-fail;git describe --dirty --always}
