Bundle-Copyright: 		MIT
Bundle-Vendor:			AlloyTools at Github
Bundle-DocURL: 			http://alloytools.org
Bundle-License:			MIT

base.version:           5.0.0
Bundle-Version:         ${base.version}.${tstamp}

-snapshot:
-groupid:               org.alloytools
-pom:                   version=${versionmask;===s;${@version}}
